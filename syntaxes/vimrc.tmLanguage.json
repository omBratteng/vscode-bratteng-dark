{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "vimrc",
  "fileTypes": [
    ".vimrc"
  ],
  "scopeName": "source.vimrc",
  "patterns": [
    {
      "name": "comment.line",
      "match": "(^| |\t)\".*$"
    },
    {
      "name": "entity.name.function",
      "match": "^( |\t)*(map|nmap|vmap|smap|xmap|omap|map!|imap|lmap|cmap)"
    },
    {
      "name": "entity.name.function",
      "match": "^( |\t)*(noremap|nnoremap|vnoremap|snoremap|xnoremap|onoremap|noremap!|inoremap|lnoremap|cnoremap)"
    },
    {
      "name": "entity.name.function",
      "match": "^( |\t)*(unmap|nunmap|vunmap|sunmap|xunmap|ounmap|unmap!|iunmap|lunmap|cunmap)"
    },
    {
      "name": "storage.type",
      "match": "^( |\t)*(set|syntax)"
    },
    {
      "name": "constant.boolean",
      "match": "( |\t)(contained|enable|list|manual|off|on|reset)"
    },
    {
      "name": "variable.other.object",
      "match": "( |\t)(aleph|nocdhome|nocursorbind|fixendofline|imactivatefunc|nolist|nopaste|report|sidescroll|tagcase|ttyfast|wincolor|noarabic|cdpath|nocursorcolumn|foldclose|noimcmdline|listchars|pastetoggle|norevins|sidescrolloff|tagfunc|ttymouse|window|arabicshape|cedit|nocursorline|foldcolumn|noimdisable|loadplugins|patchexpr|norightleft|signcolumn|taglength|ttyscroll|winheight|noallowrevins|charconvert|debug|foldenable|iminsert|magic|patchmode|noruler|nosmartcase|tagrelative|undodir|nowinfixheight|ambiwidth|nocindent|nodelcombine|foldexpr|imsearch|makeef|nopreserveindent|rulerformat|nosmartindent|tags|noundofile|nowinfixwidth|noautochdir|cinoptions|dictionary|foldignore|imstatusfunc|makeencoding|previewheight|scroll|nosmarttab|tagstack|undolevels|winminheight|noautoshelldir|clipboard|nodiff|foldlevel|includeexpr|makeprg|previewpopup|noscrollbind|softtabstop|notermbidi|undoreload|winminwidth|noautoindent|cmdheight|diffexpr|foldlevelstart|matchtime|nopreviewwindow|scrolljump|nospell|termencoding|updatecount|winwidth|noautoread|cmdwinheight|nodigraph|foldmethod|indentexpr|maxcombine|printdevice|scrolloff|spellfile|notermguicolors|updatetime|wrap|noautowrite|colorcolumn|display|foldminlines|noinfercase|maxfuncdepth|printencoding|nosecure|spelllang|termwinkey|varsofttabstop|wrapmargin|noautowriteall|columns|eadirection|foldnestmax|noinsertmode|maxmapdepth|printfont|selectmode|spelloptions|termwinsize|vartabstop|wrapscan|background|nocompatible|noedcompatible|formatexpr|isprint|maxmem|printmbcharset|shell|spellsuggest|verbose|write|nobackup|concealcursor|emoji|formatoptions|joinspaces|maxmemtot|printmbfont|shellcmdflag|nosplitbelow|textauto|verbosefile|nowriteany|backupcopy|conceallevel|encoding|formatprg|key|menuitems|printoptions|shellquote|nosplitright|notextmode|viminfofile|writebackup|backupext|completefunc|endofline|fsync|keymap|modeline|prompt|shelltemp|startofline|textwidth|virtualedit|writedelay|balloondelay|completepopup|equalalways|nogdefault|keymodel|nomodelineexpr|pumheight|shellxquote|statusline|thesaurus|novisualbell|xtermcodes|noballoonevalterm|noconfirm|equalprg|helpheight|keywordprg|modelines|pumwidth|shellxescape|suffixesadd|thesaurusfunc|warn|balloonexpr|nocopyindent|noerrorbells|helplang|langmap|modifiable|pythonthreehome|noshiftround|swapfile|notildeop|noweirdinvert|belloff|cpoptions|esckeys|nohidden|langmenu|nomodified|pyxversion|shiftwidth|swapsync|timeout|whichwrap|nobinary|cscopepathcomp|eventignore|history|nolangnoremap|more|quickfixtextfunc|switchbuf|timeoutlen|wildchar|nobomb|cscopeprg|expandtab|nohkmap|langremap|mouse|quoteescape|showbreak|synmaxcol|notitle|wildcharm|nobreakindent|cscopequickfix|noexrc|nohkmapp|laststatus|mousetime|noreadonly|noshowcmd|syntax|titlelen|wildignore|breakindentopt|nocscoperelative|fileencoding|nohlsearch|nolazyredraw|number|redrawtime|noshowfulltag|tabline|titlestring|nowildignorecase|bufhidden|nocscopetag|fileformat|noicon|nolinebreak|numberwidth|regexpengine|noshowmatch|tabpagemax|nottimeout|nowildmenu|buflisted|cscopetagorder|fileignorecase|iconstring|lines|omnifunc|norelativenumber|showmode|tabstop|ttimeoutlen|wildmode|buftype|nocscopeverbose|filetype|noignorecase|nolisp|operatorfunc|remap|showtabline|tagbsearch|ttybuiltin|wildoptions|backspace|backupdir|backupskip|breakat|casemap|cinkeys|cinscopedecls|cinwords|comments|commentstring|complete|completeopt|cryptmethod|cursorlineopt|define|diffopt|directory|errorfile|errorformat|fileencodings|fileformats|fillchars|foldmarker|foldopen|foldtext|formatlistpat|grepformat|%l%m|grepprg|guicursor|helpfile|highlight|include|indentkeys|isfname|isident|iskeyword|lispwords|matchpairs|maxmempattern|mkspellmem|mousemodel|nrformats|packpath|paragraphs|path|printexpr|printheader|rightleftcmd|runtimepath|scrollopt|sections|selection|sessionoptions|shellpipe|shellredir|shortmess|spellcapcheck|suffixes|term|termwinscroll|titleold|ttytype|viewdir|viewoptions|viminfo)"
    },
    {
      "name": "constant",
      "match": "(?i)<Leader>"
    },
    {
      "name": "constant",
      "match": "(?i)<(CR|Enter|Return)>"
    },
    {
      "name": "constant",
      "match": "(?i)<(BS|Del|Space)>"
    },
    {
      "name": "constant",
      "match": "(?i)<Esc>"
    },
    {
      "name": "constant",
      "match": "(?i)<(Up|Down|Left|Right)>"
    },
    {
      "name": "constant",
      "match": "(?i)<[CD]-.>"
    },
    {
      "name": "string.unquoted",
      "match": "(?i)[\\w-]+(\\.[\\w-]+)+"
    }
  ]
}
